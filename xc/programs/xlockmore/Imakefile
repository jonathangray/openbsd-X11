VER = xlockmore
DISTVER = xlockmore-4.07

#define IHaveSubdirs
#define PassCDebugFlags CDEBUGFLAGS="$(CDEBUGFLAGS)"

SUBDIRS = xlock modes/glx modes xmlock xalock


distclean : clean
	-rm -f Makefile config.status config.cache config.log config.h

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
