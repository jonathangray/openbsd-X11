       RGBSRCDIR = rgb
#endif
#ifndef Win32Architecture
#if HasXServer
     XINITSRCDIR = xinit
#endif
#if BuildFontServer
       XFSSRCDIR = xfs
#endif
#if BuildXKBlib
      XKBSRCDIRS = xdpyinfo xkbcomp xkbevd xkbprint xkbutils setxkbmap 
#endif
#if BuildScreenSaverExt
SCREENSAVESRCDIR = beforelight
#endif
